# Note: Buildroot's and Meson's terminologies differ about the meaning
# of 'build', 'host' and 'target':
# - Buildroot's 'host' is Meson's 'build'
# - Buildroot's 'target' is Meson's 'host'

[binaries]
c = '/mnt/work/output/bamboo/host/bin/arm-buildroot-linux-gnueabi-gcc'
cpp = '/mnt/work/output/bamboo/host/bin/arm-buildroot-linux-gnueabi-g++'
ar = '/mnt/work/output/bamboo/host/bin/arm-buildroot-linux-gnueabi-ar'
strip = '/mnt/work/output/bamboo/host/bin/arm-buildroot-linux-gnueabi-strip'
pkgconfig = '/mnt/work/output/bamboo/host/bin/pkgconf'

[properties]
needs_exe_wrapper = true
c_args = ["-D_LARGEFILE_SOURCE", "-D_LARGEFILE64_SOURCE", "-D_FILE_OFFSET_BITS=64", "-Os"@PKG_TARGET_CFLAGS@]
c_link_args = [@PKG_TARGET_CFLAGS@]
cpp_args = ["-D_LARGEFILE_SOURCE", "-D_LARGEFILE64_SOURCE", "-D_FILE_OFFSET_BITS=64", "-Os"@PKG_TARGET_CFLAGS@]
cpp_link_args = [@PKG_TARGET_CFLAGS@]
sys_root = '/mnt/work/output/bamboo/host/arm-buildroot-linux-gnueabi/sysroot'
pkg_config_libdir = '/mnt/work/output/bamboo/host/arm-buildroot-linux-gnueabi/sysroot/usr/lib/pkgconfig:/mnt/work/output/bamboo/host/arm-buildroot-linux-gnueabi/sysroot/usr/share/pkgconfig'
pkg_config_static = 'false'

[host_machine]
system = 'linux'
cpu_family ='arm'
cpu = 'arm926ej-s'
endian = 'little'
